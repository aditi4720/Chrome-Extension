{
    "name": "To Do List",
    "version": "1.0",
    "description": "A Simple To Do List",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html"
      },
    "background":["main.js"],
    "permissions": [
          "contextMenus",
          "activeTab",
          "storage",
          "notifications",
          "cookies",
          "management"
        ],
    "content_security_policy": "script-src 'self' https://kit.fontawesome.com/5744ad0b75.js; object-src 'self'"
      
  }